{
  "schemaVersion": 1,
  "id": "steveslib",
  "version": "${version}",
  "name": "${name}",
  "description": "${description}",
  "authors": [
    "${authors}"
  ],
  "contact": {
    "issues": "${issues}"
  },
  "license": "${license}",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "me.ste.library.fabric.StevesLibFabric"
      }
    ]
  },
  "mixins": [
    "steveslib.mixins.json",
    "steveslib-common.mixins.json"
  ],
  "accessWidener": "steveslib.accesswidener",
  "depends": {
    "fabricloader": ">=0.14.19",
    "fabric-api": ">=0.58.0+1.19.2",
    "minecraft": ">=1.19.2",
    "fabric-language-kotlin": ">=1.9.4+kotlin.1.8.21"
  }
}
